<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#343434"
    tools:context=".activities.one_vehicle_details_page">

    <View
        android:id="@+id/view2"
        android:layout_width="401dp"
        android:layout_height="294dp"
        android:background="@drawable/design11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="346dp"
        android:layout_height="96dp"
        android:layout_marginTop="24dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view2">

        <TextView
            android:id="@+id/textView16"
            android:layout_width="match_parent"
            android:layout_height="46dp"
            android:fontFamily="@font/cherry_swash"
            android:text="Vehicle Details"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="24sp"
            android:textStyle="bold" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tRegNum"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_weight="1"
                android:text="TextView"
                android:textAlignment="center"
                android:textSize="34sp"
                android:textStyle="bold" />
        </LinearLayout>
    </LinearLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="420dp"
        android:layout_height="624dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <ScrollView
            android:id="@+id/scrollView3"
            android:layout_width="362dp"
            android:layout_height="603dp"
            android:layout_marginStart="26dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="24dp"
            android:layout_marginBottom="17dp"
            android:background="@drawable/white_background"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginRight="40dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView28"
                    android:layout_width="match_parent"
                    android:layout_height="39dp" />

                <TextView
                    android:id="@+id/textView72"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#D1D3D3"
                    android:text="Vehicle Model   :"
                    android:textColor="#000000"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tVModel"
                    android:layout_width="match_parent"
                    android:layout_height="44dp"
                    android:text="TextView"
                    android:textAlignment="center"
                    android:textColor="#009688"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView54"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#D1D3D3"
                    android:text="Number Of Seats   :"
                    android:textColor="#040404"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tVSeats"
                    android:layout_width="match_parent"
                    android:layout_height="42dp"
                    android:text="TextView"
                    android:textAlignment="center"
                    android:textColor="#009688"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView77"
                    android:layout_width="match_parent"
                    android:layout_height="29dp"
                    android:background="#D1D3D3"
                    android:text="Phone Number  :"
                    android:textColor="#000000"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tVPhoneN"
                    android:layout_width="match_parent"
                    android:layout_height="38dp"
                    android:text="TextView"
                    android:textAlignment="center"
                    android:textColor="#009688"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView74"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#D1D3D3"
                    android:text="Vehicle Type  :"
                    android:textColor="#000000"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tVVehicleType"
                    android:layout_width="match_parent"
                    android:layout_height="38dp"
                    android:text="TextView"
                    android:textAlignment="center"
                    android:textColor="#009688"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView55"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#D1D3D3"
                    android:text="Starting Point  :"
                    android:textColor="#000000"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tVStartPoint"
                    android:layout_width="match_parent"
                    android:layout_height="41dp"
                    android:text="TextView"
                    android:textAlignment="center"
                    android:textColor="#009688"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView71"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#D1D3D3"
                    android:text="Destination  :"
                    android:textColor="#000000"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tVDestination"
                    android:layout_width="match_parent"
                    android:layout_height="37dp"
                    android:text="TextView"
                    android:textAlignment="center"
                    android:textColor="#009688"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView59"
                    android:layout_width="match_parent"
                    android:layout_height="56dp"
                    android:background="#D1D3D3"
                    android:text="Starting time from the starting point  (Morning)  :"
                    android:textColor="#000000"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tVStartTimeMor"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:text="TextView"
                    android:textAlignment="center"
                    android:textColor="#009688"
                    android:textSize="20sp"
                    android:textStyle="normal|bold" />

                <TextView
                    android:id="@+id/textView61"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#D1D3D3"
                    android:text="Arrival time to the destination  (Morning)  :"
                    android:textColor="#000000"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tVArrivalTimeMor"
                    android:layout_width="match_parent"
                    android:layout_height="42dp"
                    android:text="TextView"
                    android:textAlignment="center"
                    android:textColor="#009688"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView63"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#D1D3D3"
                    android:text="Departure time (Evening)  :"
                    android:textColor="#000000"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tVStartTimeEve"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:text="TextView"
                    android:textAlignment="center"
                    android:textColor="#009688"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView65"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#D1D3D3"
                    android:text="Arrival time to the destination  (Evening)  :"
                    android:textColor="#000000"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tVArrivalTimeEve"
                    android:layout_width="match_parent"
                    android:layout_height="44dp"
                    android:text="TextView"
                    android:textAlignment="center"
                    android:textColor="#009688"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView67"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#D1D3D3"
                    android:text="Vehicle Route  :"
                    android:textColor="#000000"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tVRoute"
                    android:layout_width="match_parent"
                    android:layout_height="78dp"
                    android:text="TextView"
                    android:textAlignment="center"
                    android:textColor="#009688"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <Button
                    android:id="@+id/btnVehicleUpdate"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:backgroundTint="#0B423C"
                    android:text="Update"
                    android:textColor="#FCFDFD"
                    android:textSize="18sp"
                    android:textStyle="bold"
                    app:cornerRadius="18dp" />

                <Button
                    android:id="@+id/btnVehicleDelete"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:backgroundTint="#0B423C"
                    android:text="Delete"
                    android:textColor="#FFFFFF"
                    android:textSize="18sp"
                    android:textStyle="bold"
                    app:cornerRadius="18dp" />

                <TextView
                    android:id="@+id/textView73"
                    android:layout_width="match_parent"
                    android:layout_height="51dp" />

            </LinearLayout>
        </ScrollView>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/btnBack22"
        android:layout_width="33dp"
        android:layout_height="45dp"
        android:layout_marginTop="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.007"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_arrow_back_ios_new_24" />

</androidx.constraintlayout.widget.ConstraintLayout>
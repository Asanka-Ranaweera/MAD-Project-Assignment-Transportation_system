<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#343434"
    tools:context=".activities.CustomerRequest_HomeView">

    <View
        android:id="@+id/view4"
        android:layout_width="384dp"
        android:layout_height="313dp"
        android:background="@drawable/design11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView47"
        android:layout_width="412dp"
        android:layout_height="102dp"
        android:layout_marginTop="40dp"
        android:text="Customer Request"
        android:textAlignment="center"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ScrollView
        android:id="@+id/scrollView5"
        android:layout_width="327dp"
        android:layout_height="499dp"
        android:layout_marginTop="120dp"
        android:background="@drawable/white_background"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView47">

    </ScrollView>

    <LinearLayout
        android:layout_width="297dp"
        android:layout_height="548dp"
        android:layout_marginTop="30dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="@+id/scrollView5"
        app:layout_constraintStart_toStartOf="@+id/scrollView5"
        app:layout_constraintTop_toTopOf="@+id/scrollView5">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recycView"
            android:layout_width="match_parent"
            android:layout_height="466dp" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>